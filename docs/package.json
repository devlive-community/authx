{
  "name": "bootstack",
  "version": "1.0.0",
  "description": "BootStack Website",
  "keywords": [
    "mkdocs",
    "documentation",
    "theme"
  ],
  "homepage": "https://bootstack.datahall.cn/",
  "license": "Apache",
  "private": true,
  "author": {
    "name": "qianmoQ",
